{
  "name": "clean-node-boilerplate",
  "author": "Artem Sinkov <sinkov@nxweb.ru>",
  "license": "BSD-3-Clause",
  "version": "0.1.0",
  "scripts": {
    "build": "babel ./src -d ./dist && webpack --config ./webpack.config.js",
    "serve": "nodemon --exec babel-node -- ./src/index.js",
    "start": "node ./dist/index.js",
    "test:unit": "jest --config ./jest.config.js",
    "lint": "eslint --no-fix ./src"
  },
  "dependencies": {
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.3.1",
    "nodemon": "^1.19.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  }
}
